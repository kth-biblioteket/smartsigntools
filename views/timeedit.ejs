<html>

<head>
    <%- include('partials/timeedithead'); %>
</head>

<body>
  <div class="App">
    <% if (!smartsignconfig.kiosk) { %>
    <div class="App-title App-padding content-top">
      <div class="titleimage">
        <img src="/smartsigntools/images/biblioteket_idag_1000_900.jpg" alt="logo"/>
      </div>
      <div class="kthlogo">
        <img src="/smartsigntools/images/kthlogo_vit.svg">
      </div>
      <div class="kthname">KTH Library</div>
      <div class="rubrik">The library today</div>
    </div>
    <% } %>
    <div id="App-content" class="App-content content-padding">
      <div id="lokalbokningar" class="lokalbokningar">
      </div>
    </div>
    <div class="App-footer content-padding">
    </div>
    <div class="iframewrap" style="padding: 0; overflow: hidden;position: absolute;bottom:50px;text-align: center;z-index: 10000; width:100%; font-size: 40px;font-weight: 700;">
        <div>
          New books at KTH Library
        </div>
        <iframe style="border: 0;height:200px;width:1000px" src="https://api.lib.kth.se/almatools/v1/newbookscarouselpage?maxshelfheight=150&nroftitlestoshow=30&minpublicationdate=0&stepInterval=3000&stepDuration=1000&carouseltype=carousel&nocoverfontsize=6&lang=sv"></iframe>
    </div>
  </div>
  <%- include('partials/timeeditscript'); %>
</body>

</html>